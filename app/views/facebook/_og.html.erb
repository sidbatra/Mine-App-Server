<meta property="og:title" content="<%=title%>"/>
<meta property="og:type" content="<%=type%>" />
<meta property="og:url" content="<%=url%>"/>
<meta property="og:locale" content="en_US"/>
<meta property="og:site_name" content="<%=CONFIG[:name]%>"/>
<meta property="og:description" content ="<%=description%>"/>
<meta property="fb:app_id" content="<%=CONFIG[:fb_app_id]%>"/>
<meta property="og:image" content="<%=image%>"/>
<%-if defined?(products)-%>
<%-products.each do |product|-%>
<meta property="<%=CONFIG[:fb_app_namespace]%>:item" content="<%=product_url(product.user.handle,product.handle,:src => 'fb')%>"/>
<%-end-%>
<%-end-%>
<%-if defined?(store) && store.present?-%>
<meta property="<%=CONFIG[:fb_app_namespace]%>:store" content="<%=store_url(store.handle,:src => 'fb')%>"/>
<%-end-%>
