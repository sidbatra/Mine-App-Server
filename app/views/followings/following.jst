<%if(!isSuggested) {%>
  <%if(isFollowing) {%>
    <a href="javascript:void(0)" 
        class="follow_on" 
        id="destroy_following_<%=userID%>"></a>
  <%} else {%>
    <a href="javascript:void(0)" 
        class="follow_off" 
        id="create_following_<%=userID%>"></a>
  <%}%>
<%} else {%>
  <%if(isFollowing) {%>
    <a href="javascript:void(0)" id="destroy_following_<%=userID%>">
      <div class="checkmark_09_666666"></div> Following
    </a>
  <%} else {%>
    <a href="javascript:void(0)" id="create_following_<%=userID%>">
      Follow
    </a>
  <%}%>
<%}%>
