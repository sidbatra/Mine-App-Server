<%-cache @key do-%>
  <%-if @filter == :user%>
    <%=@products.to_json(
        :only     => [:id,:is_gift,:handle,
                        :user_id],
        :methods  => [:thumbnail_url],
        :include  => {
          :store  => {
            :only     => [:id,:name,:handle],
            :methods  => [:is_top,:favicon_url]},
          :user => {
            :only => [:id,:handle]}})-%>
  <%-end-%>
<%-end-%>
