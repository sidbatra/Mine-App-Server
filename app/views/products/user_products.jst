<%if(products.length > 0) {%>
  <%products.each(function(product){%>
    <%=Denwen.JST['products/product']({type:'user',product:product,src:'profile'})%>
  <%});%>
<%} else if(helpers.isCurrentUser(ownerID)) {%>
  <img src="<%=helpers.imagePath('onboarding.jpg')%>"/>
<%} else {%>
  <img src="<%=helpers.imagePath('onboarding_empty.jpg')%>"/>
<%}%>
<%=helpers.blocker()%>
