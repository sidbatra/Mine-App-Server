<div><!--Container for making horizontal scrolling work-->
  <div class="row">

    <a class="prev">Previous</a>
    <a class="next_arrow">Next</a>

    <div id="products">
      <%if(shelf.get('products').length > 0) {%>
        <%shelf.get('products').each(function(product){%>
          <%=Denwen.JST['products/product']({type:'user',product:product,src:'profile'})%>
        <%});%>
      <%} else {%>
        EMPTY
      <%}%>
      <!--%=helpers.blocker()%-->
    </div>
  </div>
</div>
