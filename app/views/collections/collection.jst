<div class='titlebar timeline <%=!index ? 'latest' : ''%>'>

	<span class="trans_80">
	  <%if(index){%>
	    <%=jQuery.timeago(collection.get('created_at')).capitalize()%>
	  <%}else{%>
	    Latest
	  <%}%>
  </span>
	
	<a class='action trans_80 hover' 
      id='like_collection_<%=collection.get('id')%>' 
      href='javascript:void(0)'>
		<span class='in'>
      â™¥ <%=collection.pushed_likes_count_string()%>
    </span>
		<span class='out'>
      Like <%=collection.likes_count_string()%>
    </span>
	</a>
	
	<a class='action comment trans_80 hover' href='<%=collection.path('user_comment')%>'>
	  <span class='count'><%=collection.comments_count_string()%>&nbsp;</span>
	</a>
	
  <%if(helpers.isCurrentUser(collection.get('user_id'))){%>
    <a href='<%=collection.editPath('user_list')%>' class='edit_ffffff_14 trans_80 hover'>
      Edit
    </a>
  <%}%>
	
</div>

<%if(collection.get('name')){%>
	<div class='sequence_title'>
		<a href='<%=collection.path('user')%>'>
			<%=collection.escape('name')%>
		</a>
	</div>
<%}%>

<div class='sequence'>
	<%collection.get('products').each(function(product){%>
		<a href='<%=collection.path('user')%>' class='product' style="background-image: url('<%=product.get('thumbnail_url')%>');" >
	  </a>
	<%});%>
</div>
