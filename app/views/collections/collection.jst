<li style="margin: 0 0 10px 12px; position: relative;">
	<a class="thumbnail" style="padding: 9px;" href="<%=collection.path('user')%>">
		
		<%if(collection.get('name')){%>
			<legend style="margin: 0 2px 7px;"><%=collection.escape('name')%></legend>
		<%}%>
		
		<ul class="items">
			<%collection.get('products').each(function(product){%>
				<li>
					<div class="thumb-photo" style="padding: 2px; width: 102px; height: 102px;">
						<img style="max-height: 102px;" src="<%=product.get('thumbnail_url')%>" />
				  </div>
			  </li>
			<%});%>
		</ul>
		
		<div style="color: #999; margin-top: 5px;">
			<i style="opacity: 0.35" class="icon-time"></i>
		  <%if(index){%>
		    <%=jQuery.timeago(collection.get('created_at')).capitalize()%>
		  <%}else{%>
		    Latest
		  <%}%>
		</div>
		
	</a>
	
	<div class="btn-group right">
		<%if(helpers.isCurrentUser(collection.get('user_id'))){%>
			<a class="btn btn-small" href="<%=collection.editPath('user_list')%>">
		    <i class="icon-pencil"></i> Edit
		  </a>
	  <%}%>
		<a class="btn btn-small" id="like_collection_<%=collection.get('id')%>" href="javascript:void(0)">
			Like <%=collection.likes_count_string()%>
		</a>
	  <a class="btn btn-small" href="<%=collection.path('user_comment')%>">
	    <i class="icon-comment"></i> <%=collection.comments_count_string()%>
	  </a>
 	</div>
	
</li>