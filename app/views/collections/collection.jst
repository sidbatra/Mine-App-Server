<div class='titlebar timeline <%=!index ? 'latest' : ''%>'>

  <%if(index){%>
    <%=jQuery.timeago(collection.get('created_at')).capitalize()%>
  <%}else{%>
    Latest
  <%}%>
	
	<a class='action' id='like_collection_<%=collection.get('id')%>' href='javascript:void(0)'>
		Like <span class='count'><%=collection.likes_count_string()%></span>
	</a>
	
	<a class='action comment' href='<%=collection.path('user_comment')%>'>
	  <span class='count'><%=collection.comments_count_string()%>&nbsp;</span>
	</a>
	<br />
	
</div>

<%if(collection.has('name')){%>
	<div class='sequence_title'>
		<a href='<%=collection.path('user')%>'>
			<%=collection.get('name')%>
		</a>
	</div>
<%}%>

<div class='sequence'>
	<%collection.get('products').each(function(product){%>
		<a href='<%=collection.path('user')%>' class='product' style="background-image: url('<%=product.get('thumbnail_url')%>');" >
	  </a>
	<%});%>
</div>
