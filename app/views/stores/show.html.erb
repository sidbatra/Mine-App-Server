<div id='profile_container'>

	<!-- Right column -->
	<div id='profile_right'>
	
  	<div id='top_shoppers_box' class='box tiled'>
  	</div>
  	
		<div id='top_products' class='box'>
		</div>
	
	</div>
	
	
	<!-- Left column -->
	<div id='profile_main'>

		<div id='profile_details'>
      
      <img class='profile_user_photo slim_shadow_dark' src='<%=@store.large_url%>' />
		
			<div id='profile_details_right'>
			
		    <div id='profile_username'>
		      <%=@store.name%>
		    </div>
		
		    <div id='profile_bio'>
		      <div id='user_byline_default'>	
           	<span id='user_byline_text'>
           		Recently bought items
           	</span>
		      </div>
		    </div> 
		      
		  </div> <!--profile_details_right-->

		</div> <!--profile_details-->
		
		<div id='closet'>
     	<div class='top'>
    	</div>
      
	    <div class='rows'>
	      <div id='products'>
	      </div>
	    </div> 
      
    	<div class='bottom'>
      </div>
    </div>
    <!-- end of closet -->
	
	</div> <!-- profile_main -->

</div> <!-- profile_container -->





<%=@store.products_price%> <br>
  <%Category.fetch_all.each do |category|%>
    <%=@store.products_category_count(category.id)%>
  <%end%>






<%content_for :javascript do%>
  <%=render :partial => "misc/feedback"%>

  <script type='text/javascript'>

    new Denwen.Views.Stores.Show({
          storeJSON    : <%=@store.to_json()%>,
          source       : "<%=@source%>"});

  </script>
<%end%>
