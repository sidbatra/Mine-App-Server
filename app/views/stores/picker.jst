<a class="picker_choice unselectable" id="store_picker_<%=store.get('id')%>"> 
	
	<div class="shadowbox"></div>

	<div class="button_picker" id="button_picker_<%=store.get('id')%>">
	</div>
	
	<div class="left">
		<img src="<%=store.get('medium_url')%>" class="left_img"/>
	</div>

	<div class="right">
		<%=helpers.truncate(store.get('name'),30,".")%>
	</div>

</a> <!-- store picker -->
