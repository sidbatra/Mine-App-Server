<div class="container well" id="container">
  <table>
    <%@collections.each do |collection|%>
      <tr>
        <td><%=link_to collection.name.present? ? collection.name : 'No Name',
                  collection_path(collection.user.handle,collection.handle)%></td>
        <td><%=user_full_name(collection.user)%></td>
        <td><%=time_ago_in_words collection.created_at%></td>
      </tr>
    <%end%>
  </table>
</div>
