
<div id="container">
  <ul>
    <%@grouped_users.each do |age,users|%>
      <ul>
      <p><b><%=age*5%>--<%=(age+1)*5-1%></b></p>
      <%users.each do |user|%>
        <li><%=link_to h(user_full_name(user)), user_path(user) if user %></li>
      <%end%>
      </ul>
      <br><br><br>
    <%end%>
  </ul>
</div>
