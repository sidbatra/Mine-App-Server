<div class="actions-header">
  <%=header%> 
  <a class="right" data-toggle="modal" href="#modal-user-friends">
  	View all
  </a>
</div> <!-- /actions-header -->

<ul class="thumbnails">

	<%if(!count){%>
	
	  <li>
			<a class="thumbnail">
				invite cells here
			</a>
	  </li>
	  
	<%}else{%>
	
	  <%users.each(function(user){%>
	  	<li>
	    	<%=Denwen.JST['users/tiled_list']({user:user, src:src + '_list'})%>
	    </li>
	  <%});%>
	
	<%}%>
	
</ul> <!-- /thumbnails -->

<div class="modal fade" id="modal-user-friends">
  <div class="modal-header">
    <a class="close" data-dismiss="modal">Ã—</a>
    <h3>Influenced by</h3>
  </div>
  <div class="modal-body">
  
  	<ul class="thumbnails">
	  	<%users.each(function(user){%>
		  	<li>
		    	<%=Denwen.JST['users/tiled_list']({user:user, src:src + '_list'})%>
		    </li>
		  <%});%>
		</ul> <!-- /modal thumbnails -->
		
  </div>
</div> <!-- /modal -->
